{
  "compilerOptions": {
    "target": "ES2018",
    "module": "ESNext",
    "moduleResolution": "Node",
    "lib": ["ESNext", "ESNext.AsyncIterable", "DOM"],
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": false,
    "noEmit": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./src/*"],
      "@/*": ["./src/*"]
    },
    "types": [
      "@nuxt/types",
      "@nuxtjs/i18n",
      "@nuxtjs/axios",
      "@nuxtjs/auth-next",
      "@nuxtjs/vuetify",
      "@types/node",
    ]
  },
  "vueCompilerOptions": {
    "target": 2.7
  },
  "exclude": ["node_modules", ".nuxt", "dist"]
}
